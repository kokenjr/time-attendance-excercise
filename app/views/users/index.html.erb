<h1>Users</h1>

<h2><%= link_to "Add User", new_user_path %></h2> <br />

<% unless @users.blank? %>
  <% @users.each do |user| %>
    <li><%= user.employee_id %> (<%= user.first_name %> <%= user.last_name %>)
            <%= link_to "Delete", user, :method => :delete %>
    </li>
  <% end %>
<% end %>
